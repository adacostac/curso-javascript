{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 3
    },
    "env": {
        "commonjs": true,
        "mocha": true,
        "browser": true
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console" : "off"
    }
}
